{
  "name": "farmpro-api",
  "version": "1.0.0",
  "main": "dist/main.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"contracts/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "test:database": "jest --config ./jest.database.config.js",
    "test:database:watch": "jest --config ./jest.database.config.js --watch",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "contracts:install": "cd contracts && npm install",
    "contracts:build": "cd contracts && npm run build",
    "contracts:build:watch": "cd contracts && npm run build:watch",
    "contracts:clean": "cd contracts && npm run clean",
    "contracts:type-check": "cd contracts && npm run type-check",
    "contracts:type-check:strict": "cd contracts && npm run type-check:strict",
    "contracts:lint": "cd contracts && npm run lint",
    "contracts:lint:fix": "cd contracts && npm run lint:fix",
    "contracts:test:types": "cd contracts && npm run test:types",
    "contracts:test:runtime": "cd contracts && npm run test:runtime",
    "contracts:validate": "cd contracts && npm run validate",
    "contracts:prepublish": "cd contracts && npm run prepublishOnly",
    "contracts:publish": "cd contracts && npm publish",
    "contracts:version": "cd contracts && npm version",
    "contracts:all": "npm run contracts:install && npm run contracts:build && npm run contracts:validate",
    "contracts:publish:build": "node scripts/publish-contracts.js build",
    "contracts:publish:validate": "node scripts/publish-contracts.js validate",
    "contracts:publish:version": "node scripts/publish-contracts.js version",
    "contracts:publish:github": "node scripts/publish-contracts.js github",
    "contracts:publish:npm": "node scripts/publish-contracts.js npm",
    "contracts:publish:all": "node scripts/publish-contracts.js all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@getbrevo/brevo": "^3.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.6",
    "@node-rs/argon2": "^2.0.2",
    "@prisma/client": "^6.15.0",
    "@ts-rest/core": "^3.53.0-rc.1",
    "@ts-rest/nest": "^3.53.0-rc.1",
    "@types/multer": "^2.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.11.0",
    "cache-manager": "^7.2.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "openai": "^5.20.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.15.0",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "@testcontainers/postgresql": "^11.5.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.1",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "testcontainers": "^11.5.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
